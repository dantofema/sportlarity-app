{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "bezhansalleh/filament-shield": "^4.0",
        "filament/filament": "^4.0",
        "flowframe/laravel-trend": "^0.4.0",
        "laravel/framework": "^12.9",
        "laravel/horizon": "^5.44",
        "laravel/nightwatch": "^1.22",
        "laravel/tinker": "^2.9",
        "owenvoke/blade-fontawesome": "^2.6",
        "pxlrbt/filament-excel": "^3.0",
        "sentry/sentry-laravel": "^4.6"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^2.0",
        "laravel/pint": "^1.27",
        "laravel/sail": "^1.52",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.2",
        "pestphp/pest-plugin-faker": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.1",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "check": [
            "@pint",
            "@rector",
            "@phpstan",
            "@test"
        ],
        "pint": "./vendor/bin/sail bin pint",
        "rector": "./vendor/bin/sail bin rector",
        "phpstan": "./vendor/bin/sail bin phpstan analyse",
        "test": "./vendor/bin/sail test"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
